summary: Edge test plan
discover:
  how: fmf
  test: edge-test
execute:
  how: tmt
provision:
  hardware:
    virtualization:
      is-supported: true
    cpu:
      processors: ">= 2"
    memory: ">= 6 GB"

/edge-commit:
  summary: Test edge commit
  environment+:
    TEST_CASE: edge-commit
  adjust+:
    - when: arch != x86_64
      enabled: false

